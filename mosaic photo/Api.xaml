<Window x:Class="mosaic_photo.Api"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mosaic_photo"
        mc:Ignorable="d"
        Title="개인 API 입력" Height="200" Width="300" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="10">
            <TextBlock>Microsoft Computer Vision API</TextBlock>
            <TextBox x:Name="ms_api" Width="250" Height="20" HorizontalAlignment="Left" Margin="0,5,0,0"></TextBox>
            <TextBlock Margin="0,5,0,0">Flickr API</TextBlock>
            <TextBox x:Name="flickr_api" Width="250" Height="20" HorizontalAlignment="Left" Margin="0,5,0,0"></TextBox>
            <StackPanel Orientation="Horizontal" Margin="0,20,0,0" HorizontalAlignment="Center">
                <Button x:Name="btnOk" Content="확인" Width="100" Click="btnOk_Click"/>
                <Button x:Name="btnCancel" Content="취소" Width="100" Margin="30,0,0,0" Click="btnCancel_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
